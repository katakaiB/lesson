data segment
num db ? ; значение для переменной 'a'
denom db ? ; значение для переменной 'c'
exp db ? ; значение для переменной 'b'
endata

code segment
start:
mov ax, data ; загружаем данные из сегмента данных
mov dx, [num] ; загружаем значение для переменной 'a'
mov cx, [denom] ; загружаем значение для переменной 'c'
mov bx, [exp] ; загружаем значение для переменной 'b'

; вычисление значений подвыражений
; sub dx, bx ; вычитание степени
; mul cx ; умножение на числитель
; div ax ; деление на знаменатель

; результат будет в регистре AX
mov dx, 0 ; очищаем регистр DX перед возвратом результата
ret ; возврат результата в стек
endcode

end start
